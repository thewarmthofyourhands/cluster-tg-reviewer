{
  "type": "object",
  "properties": {
    "token": {
      "type": "string",
      "description": "Token",
      "example": "asfasfa2112rf13"
    },
    "repositoryFullName": {
      "type": "string",
      "minLength": 3,
      "pattern": "^[a-zA-Z0-9_-]+/[a-zA-Z0-9_-]+$",
      "description": "Repository full name",
      "example": "organization/repository-name"
    }
  },
  "required": [
    "token",
    "repositoryFullName"
  ],
  "additionalProperties": false
}
